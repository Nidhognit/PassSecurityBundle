{
  "name": "nidhognit/pass-security-bundle",
  "type": "symfony-bundle",
  "description": "Symfony PassSecurityBundle",
  "keywords": ["UValidate password"],
  "homepage": "https://github.com/Nidhognit/PassSecurityBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Dima"
    }
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "symfony/console": "^2.7 || ^3.0",
    "symfony/dependency-injection": "^2.7 || ^3.0",
    "symfony/config": "^2.7 || ^3.0",
    "symfony/yaml": "^2.7 || ^3.0",
    "doctrine/orm": "^2.3",
    "doctrine/doctrine-bundle": "^1.6",
    "incenteev/composer-parameter-handler": "^2.0"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Nidhognit\\PassSecurityBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "support": {
    "docs": "https://github.com/Nidhognit/PassSecurityBundle/issues"
  }

}
